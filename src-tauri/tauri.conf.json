{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Nature's Diet Marketing",
  "version": "0.1.0",
  "identifier": "com.naturesdiet.marketing",
  "build": {
    "frontendDist": "../frontend/dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "cd ../frontend && npm run dev",
    "beforeBuildCommand": "cd /Users/e/Documents/GUNS/ENIDSASSETS/NaturesDietMarketingTeam/frontend && npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Nature's Diet Marketing Tool",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true

  },
  "plugins": {},
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
